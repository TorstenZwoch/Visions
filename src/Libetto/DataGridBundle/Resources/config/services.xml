    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="poe_grid.class">Libetto\DataGridBundle\Grid\Grid</parameter>
    </parameters>
    <services>
        <service id="poe_grid" class="%poe_grid.class%">
            <argument type="service" id="service_container" />
        </service>
    </services>
</container>
