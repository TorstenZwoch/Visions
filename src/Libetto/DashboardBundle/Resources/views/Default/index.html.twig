{% extends '::base.html.twig' %}

{% block body %}
    {% if username == 'anon.' %}
        <h1>{% trans %}dashboard.default.messages.no login{% endtrans %}</h1>
            {% trans %}dashboard.default.messages.please log in{% endtrans %} 
            <a href="{{ path('fos_user_security_login') }}">
            {% trans %}dashboard.default.action.login{% endtrans %}</a>
    {% else %}
        {% trans with {'%name%': username} %} dashboard.default.messages.welcome{% endtrans%}
    {%endif%}
{% endblock %}